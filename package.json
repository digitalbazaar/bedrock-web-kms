{
  "name": "bedrock-web-kms",
  "version": "2.0.1-0",
  "description": "Web app APIs for Bedrock Key Management",
  "main": "index.js",
  "dependencies": {
    "axios": "^0.18.0",
    "base64url-universal": "^1.0.0",
    "bedrock-web-store": "^1.1.1",
    "crypto-ld": "^3.4.0",
    "jsonld-signatures": "^4.0.0",
    "ocapld": "^1.1.0",
    "tweetnacl": "^1.0.1",
    "uuid-random": "^1.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/digitalbazaar/bedrock-web-kms"
  },
  "keywords": [
    "bedrock"
  ],
  "author": {
    "name": "Digital Bazaar, Inc.",
    "email": "support@digitalbazaar.com",
    "url": "https://digitalbazaar.com"
  },
  "bugs": {
    "url": "https://github.com/digitalbazaar/bedrock-web-kms/issues"
  },
  "homepage": "https://github.com/digitalbazaar/bedrock-web-kms",
  "bedrock": {
    "browserDependencies": "all",
    "manifest": {
      "axios": {
        "main": "dist/axios.min.js"
      },
      "base64url-universal": {
        "bedrock": {
          "browserDependencies": []
        },
        "browser": "lib/browser.js"
      },
      "crypto-ld": {
        "bedrock": {
          "browserDependencies": [
            "base64url-universal",
            "node-forge"
          ]
        },
        "browser": "lib/index.js"
      },
      "jsonld": {
        "main": "dist/jsonld.js"
      },
      "jsonld-signatures": {
        "main": "dist/jsonld-signatures.js"
      },
      "ocapld": {
        "main": "dist/ocapld.js"
      }
    },
    "systemjs": {
      "packages": {
        "crypto-ld": {
          "main": "lib/index.js",
          "defaultExtension": "js",
          "map": {
            "bs58": "@empty",
            "crypto": "@empty",
            "sodium-native": "@empty",
            "util": "@empty"
          }
        }
      }
    }
  }
}
